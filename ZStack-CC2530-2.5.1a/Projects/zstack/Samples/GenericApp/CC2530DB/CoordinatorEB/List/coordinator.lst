    579           */
    580          static void GenericApp_ProcessRtosMessage( void )
    581          {
    582            osalQueue_t inMsg;
    583          
    584            if ( osal_queue_receive( OsalQueue, &inMsg, 0 ) == pdPASS )
    585            {
    586              uint8 cmndId = inMsg.cmnd;
    587              uint32 counter = osal_build_uint32( inMsg.cbuf, 4 );
    588          
    589              switch ( cmndId )
    590              {
    591                case CMD_INCR:
    592                  counter += 1;  /* Increment the incoming counter */
    593                                 /* Intentionally fall through next case */
    594          
    595                case CMD_ECHO:
    596                {
    597                  userQueue_t outMsg;
    598          
    599                  outMsg.resp = RSP_CODE | cmndId;  /* Response ID */
    600                  osal_buffer_uint32( outMsg.rbuf, counter );    /* Increment counter */
    601                  osal_queue_send( UserQueue1, &outMsg, 0 );  /* Send back to UserTask */
    602                  break;
    603                }
    604                
    605                default:
    606                  break;  /* Ignore unknown command */    
    607              }
    608            }
